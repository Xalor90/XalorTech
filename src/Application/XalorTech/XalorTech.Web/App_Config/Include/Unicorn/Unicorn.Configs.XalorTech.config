<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
	<sitecore>
		<unicorn>
			<configurations>
				<configuration name="XalorTech" description="Templates, Layouts, etc. for XalorTech Web App">
					<predicate>
						<include name="Layouts" database="master" path="/sitecore/layout/Layouts/Project/XalorTech" />
						<include name="Templates" database="master" path="/sitecore/templates/Project/XalorTech" />
					</predicate>
					<dataProviderConfiguration enableTransparentSync="true" />
					<syncConfiguration updateLinkDatabase="false" updateSearchIndex="false" />
				</configuration>
			</configurations>
		</unicorn>
	</sitecore>
</configuration>
